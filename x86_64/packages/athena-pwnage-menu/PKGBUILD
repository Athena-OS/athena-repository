pkgname=athena-pwnage-menu
pkgver=1.0.0
pkgrel=1
pkgdesc="PWNage Menu for Athena"
arch=('any')
license=("custom")
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/usr/share/icons/pwnage"

  cp -rf "${srcdir}"/usr/share/icons/pwnage/* "${pkgdir}/usr/share/icons/pwnage/"
}
sha256sums=('288ba5778f4d5ce55e440a555e07e6ce8cc5e8f27d5a902e19e77a3a6d104aba')
