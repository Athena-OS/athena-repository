pkgname=athena-nightab
pkgver=1.0.0
pkgrel=2
pkgdesc="Nightab browser themes for Athena"
arch=('any')
license=("custom")
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/usr/share/nightab"

  cp -rf "${srcdir}/usr/share/nightab/athena_astronaut.json" "${pkgdir}/usr/share/nightab/"
  cp -rf "${srcdir}/usr/share/nightab/athena_persona_5.json" "${pkgdir}/usr/share/nightab/"
  
}
sha256sums=('06dd24caa95d1c97867e5e707cca33eaf053b9acae2745742e88ad6d47abb064')
