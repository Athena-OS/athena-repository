pkgname=athena-zsh
pkgver=1.0.0
pkgrel=2
pkgdesc="Zsh for Athena"
arch=('any')
license=("custom")
depends=('zsh' 'powerline-fonts')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.zshrc" "${pkgdir}/etc/skel/"
  
}
sha256sums=('7f04f181389d4c6f74e65173aec5dcd35d347006bc9b6da5573fe831244f11d1')
