pkgname=athena-zsh
pkgver=1.0.0
pkgrel=1
pkgdesc="Zsh for Athena"
arch=('any')
license=("custom")
depends=('zsh' 'powerline-fonts')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.zshrc" "${pkgdir}/etc/skel/"
  
}
sha256sums=('a00557e2f79c5235bbb015e708f709659f0cae0854ff4e0d7438dab0a214302a')
