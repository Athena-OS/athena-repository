pkgname=athena-zsh
pkgver=1.0.0
pkgrel=1
pkgdesc="Zsh for Athena"
arch=('x86_64')
license=("custom")
depends=('zsh' 'powerline-fonts')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.zshrc" "${pkgdir}/etc/skel/"
  
}
sha256sums=('9573c6945e5a0eb8de5e0186cc7385cef4c9c413870f92bd796cab03788cf094')
