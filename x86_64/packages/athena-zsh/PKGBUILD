pkgname=athena-zsh
pkgver=1.0.0
pkgrel=1
pkgdesc="Zsh for Athena"
arch=('x86_64')
license=("custom")
depends=('zsh' 'powerline-fonts')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.zshrc" "${pkgdir}/etc/skel/"
  
}
sha256sums=('134efec4c1f12369c16ee3697ab98ea1ca948477311b492dcb0f7fe0e47af4ed')
