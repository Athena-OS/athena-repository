pkgname=athena-zsh
pkgver=1.0.0
pkgrel=1
pkgdesc="Zsh for Athena"
arch=('x86_64')
license=("custom")
depends=('zsh' 'powerline-fonts')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.zshrc" "${pkgdir}/etc/skel/"
  
}
sha256sums=('96ab63d19bfad7c8644f00bbdb53ca2726d2edd41ef0eca1fe3bd82c771fc26d')
