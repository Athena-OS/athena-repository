pkgname="athena-zsh"
pkgver="1.0.0"
pkgrel="1"
pkgdesc="Zsh for Athena"
arch=("x86_64")
license=("custom")
depends=('zsh' 'powerline-fonts')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.zshrc" "${pkgdir}/etc/skel/"
  
}
sha256sums=('86525acb6305ceb6011c7a089e84eff86730f1222518f4eae7315d73b931b21a')
