pkgname=athena-fish
pkgver=1.0.0
pkgrel=1
pkgdesc="Fish for Athena"
arch=('x86_64')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('ae6fd3d408f78a7f5b9be4058f5ee4a85cb295ab8ebca814a5d7ee0325536e63')
