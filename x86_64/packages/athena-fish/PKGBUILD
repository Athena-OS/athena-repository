pkgname=athena-fish
pkgver=1.0.0
pkgrel=1
pkgdesc="Fish for Athena"
arch=('any')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('f8c8559ee4145073dae07b981960b55d73bcbe0fe78ed5260d40d1f5aee5adb6')
