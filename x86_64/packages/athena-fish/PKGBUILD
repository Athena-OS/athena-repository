pkgname=athena-fish
pkgver=1.0.0
pkgrel=1
pkgdesc="Fish for Athena"
arch=('x86_64')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('15a5ce5ea5bc8d77c7eb722ee027b56ca6070e1dadbaad2e8c79e766af292e25')
