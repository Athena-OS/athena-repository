pkgname=athena-fish
pkgver=1.0.0
pkgrel=1
pkgdesc="Fish for Athena"
arch=('x86_64')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('b9bd82811a09a5fea8fdade0080f24cd0ecb9fc16c7286179f93bdad1174b81d')
