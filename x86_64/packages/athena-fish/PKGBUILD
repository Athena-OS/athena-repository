pkgname=athena-fish
pkgver=1.0.0
pkgrel=2
pkgdesc="Fish for Athena"
arch=('any')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('a28518a7da05f12867d6a8894c5d0f2f7d15f3b44f6027798d77fe4fda13358b')
