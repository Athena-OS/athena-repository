#!/bin/bash

echo "############################################"
echo "Start Athena snapper cleanup"
echo "############################################"

if [ -f /usr/bin/snapper ]; then
	for i in {1..20};
	do 
		snapper --no-dbus delete $i
	done
fi

echo
echo "############################################"
echo "End athena-snapper"
echo "############################################"
echo
